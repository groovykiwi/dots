#!/bin/bash
if [[ -z $1 ]]; then
	value=$((1 + $RANDOM % 7))
else
	value=$1
fi
echo $value

if [[ $value == "1" ]]; then
	T='*' # The test text

	echo -e "\n             40m 41m 42m 43m\
	 44m 45m 46m 47m"

	for FGs in '    m' '   1m' '  30m' '1;30m' '  31m' '1;31m' '  32m' \
		'1;32m' '  33m' '1;33m' '  34m' '1;34m' '  35m' '1;35m' \
		'  36m' '1;36m' '  37m' '1;37m'; do
		FG=${FGs// /}
		echo -en " $FGs \033[$FG  $T  "
		for BG in 40m 41m 42m 43m 44m 45m 46m 47m; do
			echo -en "$EINS \033[$FG\033[$BG $T \033[0m"
		done
		echo
	done
	echo
fi

if [[ $value == "2" ]]; then
	xdef="$HOME/.Xresources"
	colors=($(sed -re '/^!/d; /^$/d; /^#/d; s/(\*color)([0-9]):/\10\2:/g;' $xdef | grep 'color[01][0-9]:' | sort | sed 's/^.*: *//g'))

	echo -e "\e[1;37m 
 Black    Red      Green    Yellow   Blue     Magenta   Cyan    White   
 â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\e[0m"
	for i in {0..7}; do echo -en "\e[$((30 + $i))m ${colors[i]} \e[0m"; done
	echo
	for i in {8..15}; do echo -en "\e[1;$((22 + $i))m ${colors[i]} \e[0m"; done
	echo -e "\n"
fi

if [[ $value == "3" ]]; then
	xdef="$HOME/.Xresources"

	colors=($(sed -re '/^!/d; /^$/d; /^#/d; s/(\*color)([0-9]):/\10\2:/g;' $xdef | grep 'color[01][0-9]:' | sort | sed 's/^.*: *//g')
	)

	echo
	for i in {0..7}; do echo -en "\e[$((30 + $i))m ${colors[i]} \u2588\u2588 \e[0m"; done
	echo
	for i in {8..15}; do echo -en "\e[1;$((22 + $i))m ${colors[i]} \u2588\u2588 \e[0m"; done
	echo -e "\n"
fi

if [[ $value == "4" ]]; then
	f=3 b=4
	for j in f b; do
		for i in {0..7}; do
			printf -v $j$i %b "\e[${!j}${i}m"
		done
	done
	bld=$'\e[1m'
	rst=$'\e[0m'
	inv=$'\e[7m'

	cat <<EOF
	$rst
	 $f3  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„                $f1  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    $f2  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    $f4  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    $f5  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    $f6  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    
	 $f3â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€               $f1â–„$f7â–ˆâ–€â–ˆ$f1â–ˆâ–ˆ$f7â–ˆâ–€â–ˆ$f1â–ˆâ–ˆâ–„  $f2â–„â–ˆ$f7â–ˆâ–ˆâ–ˆ$f2â–ˆâ–ˆ$f7â–ˆâ–ˆâ–ˆ$f2â–ˆâ–„  $f4â–„â–ˆ$f7â–ˆâ–ˆâ–ˆ$f4â–ˆâ–ˆ$f7â–ˆâ–ˆâ–ˆ$f4â–ˆâ–„  $f5â–„â–ˆ$f7â–ˆâ–ˆâ–ˆ$f5â–ˆâ–ˆ$f7â–ˆâ–ˆâ–ˆ$f5â–ˆâ–„  $f6â–„â–ˆâ–ˆ$f7â–ˆâ–€â–ˆ$f6â–ˆâ–ˆ$f7â–ˆâ–€â–ˆ$f6â–„
	 $f3â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€      $f7â–„â–„  â–„â–„  â–„â–„   $f1â–ˆ$f7â–„â–„â–ˆ$f1â–ˆâ–ˆ$f7â–„â–„â–ˆ$f1â–ˆâ–ˆâ–ˆ  $f2â–ˆâ–ˆ$f7â–ˆ â–ˆ$f2â–ˆâ–ˆ$f7â–ˆ â–ˆ$f2â–ˆâ–ˆ  $f4â–ˆâ–ˆ$f7â–ˆ â–ˆ$f4â–ˆâ–ˆ$f7â–ˆ â–ˆ$f4â–ˆâ–ˆ  $f5â–ˆâ–ˆ$f7â–ˆ â–ˆ$f5â–ˆâ–ˆ$f7â–ˆ â–ˆ$f5â–ˆâ–ˆ  $f6â–ˆâ–ˆâ–ˆ$f7â–ˆâ–„â–„$f6â–ˆâ–ˆ$f7â–ˆâ–„â–„$f6â–ˆ
	 $f3â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„      $f7â–€â–€  â–€â–€  â–€â–€   $f1â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  $f2â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  $f4â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  $f5â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  $f6â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  
	 $f3â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„â–„               $f1â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ  $f2â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ  $f4â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ  $f5â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ  $f6â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ
	 $f3  â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€                $f1â–€   â–€  â–€   â–€  $f2â–€   â–€  â–€   â–€  $f4â–€   â–€  â–€   â–€  $f5â–€   â–€  â–€   â–€  $f6â–€   â–€  â–€   â–€ 
	$bld
	 $f3  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„                $f1  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    $f2  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    $f4  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    $f5  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    $f6  â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„    
	 $f3â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€               $f1â–„$f7â–ˆâ–€â–ˆ$f1â–ˆâ–ˆ$f7â–ˆâ–€â–ˆ$f1â–ˆâ–ˆâ–„  $f2â–„â–ˆ$f7â–ˆ â–ˆ$f2â–ˆâ–ˆ$f7â–ˆ â–ˆ$f2â–ˆâ–„  $f4â–„â–ˆ$f7â–ˆ â–ˆ$f4â–ˆâ–ˆ$f7â–ˆ â–ˆ$f4â–ˆâ–„  $f5â–„â–ˆ$f7â–ˆ â–ˆ$f5â–ˆâ–ˆ$f7â–ˆ â–ˆ$f5â–ˆâ–„  $f6â–„â–ˆâ–ˆ$f7â–ˆâ–€â–ˆ$f6â–ˆâ–ˆ$f7â–ˆâ–€â–ˆ$f6â–„
	 $f3â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€      $f7â–„â–„  â–„â–„  â–„â–„   $f1â–ˆ$f7â–„â–„â–ˆ$f1â–ˆâ–ˆ$f7â–„â–„â–ˆ$f1â–ˆâ–ˆâ–ˆ  $f2â–ˆâ–ˆ$f7â–ˆâ–ˆâ–ˆ$f2â–ˆâ–ˆ$f7â–ˆâ–ˆâ–ˆ$f2â–ˆâ–ˆ  $f4â–ˆâ–ˆ$f7â–ˆâ–ˆâ–ˆ$f4â–ˆâ–ˆ$f7â–ˆâ–ˆâ–ˆ$f4â–ˆâ–ˆ  $f5â–ˆâ–ˆ$f7â–ˆâ–ˆâ–ˆ$f5â–ˆâ–ˆ$f7â–ˆâ–ˆâ–ˆ$f5â–ˆâ–ˆ  $f6â–ˆâ–ˆâ–ˆ$f7â–ˆâ–„â–„$f6â–ˆâ–ˆ$f7â–ˆâ–„â–„$f6â–ˆ
	 $f3â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„      $f7â–€â–€  â–€â–€  â–€â–€   $f1â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  $f2â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  $f4â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  $f5â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  $f6â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  
	 $f3â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„â–„               $f1â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ  $f2â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ  $f4â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ  $f5â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ  $f6â–ˆâ–ˆâ–€â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–€â–ˆâ–ˆ
	 $f3  â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€                $f1â–€   â–€  â–€   â–€  $f2â–€   â–€  â–€   â–€  $f4â–€   â–€  â–€   â–€  $f5â–€   â–€  â–€   â–€  $f6â–€   â–€  â–€   â–€ 
	$rst
EOF
fi

if [[ $value == "5" ]]; then
	initializeANSI() {
		esc=""

		blackf="${esc}[31m"
		redf="${esc}[31m"
		greenf="${esc}[32m"
		yellowf="${esc}[33m" bluef="${esc}[34m"
		purplef="${esc}[35m"
		cyanf="${esc}[36m"
		whitef="${esc}[37m"

		blackb="${esc}[40m"
		redb="${esc}[41m"
		greenb="${esc}[42m"
		yellowb="${esc}[43m" blueb="${esc}[44m"
		purpleb="${esc}[45m"
		cyanb="${esc}[46m"
		whiteb="${esc}[47m"

		boldon="${esc}[1m"
		boldoff="${esc}[22m"
		italicson="${esc}[3m"
		italicsoff="${esc}[23m"
		ulon="${esc}[4m"
		uloff="${esc}[24m"
		invon="${esc}[7m"
		invoff="${esc}[27m"

		reset="${esc}[0m"
	}

	initializeANSI

	cat <<EOF

	    ${boldon}${blackf} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${reset}
	   ${boldon}${blackf}â–ˆâ–ˆ${reset}â–ˆâ–ˆ${reset}${bluef}â–ˆâ–ˆ${reset}${boldon}${blackf}â–ˆâ–ˆ${reset}${bluef}â–ˆâ–ˆ${reset}
	   ${boldon}${blackf}â–ˆâ–ˆ${reset}â–ˆâ–ˆ${bluef}â–ˆâ–ˆ${reset}${boldon}${blackf}â–ˆâ–ˆ${reset}${bluef}â–ˆâ–ˆ${reset}
	   ${boldon}${blackf}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${reset}${purplef}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${reset}
	   ${boldon}${blackf}â–ˆâ–ˆâ–ˆâ–ˆ${reset}â–ˆâ–ˆâ–ˆâ–ˆ${boldon}${blackf}â–ˆâ–ˆ${reset}
	 ${boldon}${blackf}â–ˆâ–ˆâ–ˆâ–ˆ${reset}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${boldon}${blackf}â–ˆâ–ˆ${reset}
	 ${boldon}${blackf}â–ˆâ–ˆâ–ˆâ–ˆ${reset}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${boldon}${blackf}â–ˆâ–ˆ${reset}
	 ${boldon}${blackf}â–ˆâ–ˆâ–ˆâ–ˆ${reset}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${boldon}${blackf}â–ˆâ–ˆ${reset}
	   ${boldon}${blackf}â–ˆâ–ˆâ–ˆâ–ˆ${reset}â–ˆâ–ˆâ–ˆâ–ˆ${boldon}${blackf}â–ˆâ–ˆ${reset}
	  ${boldon}${blackf}â–ˆâ–ˆâ–ˆ${reset}${purplef}â–ˆâ–ˆâ–ˆ   â–ˆâ–ˆâ–ˆâ–ˆ${reset}

EOF
fi

if [[ "$value" == "6" ]]; then
	f=3 b=4
	for j in f b; do
		for i in {0..7}; do
			printf -v $j$i %b "\e[${!j}${i}m"
		done
	done
	for i in {0..7}; do
		printf -v fbright$i %b "\e[9${i}m"
	done
	bld=$'\e[1m'
	rst=$'\e[0m'
	inv=$'\e[7m'
	cat <<EOF

$f1    â–„â–„â–„      $f2    â–„â–„â–„      $f3    â–„â–„â–„      $f4    â–„â–„â–„      $f5    â–„â–„â–„      $f6    â–„â–„â–„     
$f1   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $f2   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $f3   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $f4   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $f5   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $f6   â–€â–ˆâ–€â–ˆâ–ˆ  â–„ 
$f1 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $f2 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $f3 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $f4 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $f5 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $f6 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€  
$f1    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $f2    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $f3    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $f4    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $f5    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $f6    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  
$f1       â–€â–€â–€â–€â–„ $f2       â–€â–€â–€â–€â–„ $f3       â–€â–€â–€â–€â–„ $f4       â–€â–€â–€â–€â–„ $f5       â–€â–€â–€â–€â–„ $f6       â–€â–€â–€â–€â–„
$bld
$fbright1    â–„â–„â–„      $fbright2    â–„â–„â–„      $fbright3    â–„â–„â–„      $fbright4    â–„â–„â–„      $fbright5    â–„â–„â–„      $fbright6    â–„â–„â–„     
$fbright1   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $fbright2   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $fbright3   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $fbright4   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $fbright5   â–€â–ˆâ–€â–ˆâ–ˆ  â–„  $fbright6   â–€â–ˆâ–€â–ˆâ–ˆ  â–„ 
$fbright1 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $fbright2 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $fbright3 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $fbright4 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $fbright5 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€   $fbright6 â–€â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€  
$fbright1    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $fbright2    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $fbright3    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $fbright4    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $fbright5    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ   $fbright6    â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ  
$fbright1       â–€â–€â–€â–€â–„ $fbright2       â–€â–€â–€â–€â–„ $fbright3       â–€â–€â–€â–€â–„ $fbright4       â–€â–€â–€â–€â–„ $fbright5       â–€â–€â–€â–€â–„ $fbright6       â–€â–€â–€â–€â–„
$rst
EOF
fi

if [[ "$value" == "7" ]]; then
	initializeANSI() {
		esc=""

		redf="${esc}[31m"
		greenf="${esc}[32m"
		yellowf="${esc}[33m" bluef="${esc}[34m"
		cyanf="${esc}[36m"
		purplef="${esc}[35m"

		redfbright="${esc}[91m"
		greenfbright="${esc}[92m"
		yellowfbright="${esc}[93m" bluefbright="${esc}[94m"
		cyanfbright="${esc}[96m"
		purplefbright="${esc}[95m"

		boldon="${esc}[1m"
		reset="${esc}[0m"
	}

	# note in this first use that switching colors doesn't require a reset
	# first - the new color overrides the old one.

	initializeANSI

	cat <<EOF

${boldon}${redfbright}                                          â–‘â–“â–“                              
${boldon}${redfbright}                                       â–“â–“â–“â–“â–“â–“â–“â–“â–“                                
${boldon}${redfbright}                                â–“â–‘  â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“                                 
${boldon}${redfbright}                              â–“â–“â–“  â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“                                 
${boldon}${redfbright}                        â–“    â–“â–“â–“   â–“â–’      â–‘â–“â–“â–“â–“â–“                               
